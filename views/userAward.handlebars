<link rel="stylesheet" href="css/createForm.css" type="text/css">
<div id="createAward" class="createForm">
	<form>
		<fieldset>
			<h1 class="title">Create an Award</h1>
			<p class="fields"><input id="name" name="name" type="text" placeholder="Full Name" autocomplete="on" required></p>
			<p class="fields"><input id="email" name="email" type="email" placeholder="Email Address" autocomplete="on" required></p>
			<p class="fields"><input id="department" name="department" type="type" placeholder="Department" autocomplete="on" required></p>
			<p class="fields"><input id="region" name="region" type="type" placeholder="Region" autocomplete="on" required></p>
			<p class="oddField">
				<select name="awardType" required>
					<option value="">Award Type</option>
					<option value="">Type 1</option>
					<option value="">Type 2</option>
				</select>
			</p>
			<p class="button"><input class="submit" id="createAward" type="submit" value="Create Award"></p>
		</fieldset>
	</form>
</div>
