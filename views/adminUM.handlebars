<div class="body-container">
	<h1 class="pageTitle">User Management</h1>
	<p>
		<input type="text" id="myInput" onkeyup="search()" placeholder="Search for a user..." title="User Search Bar">
<a href="/add-user">Add User</a></p>
	<div class="users">
		{{#if results}}
			<table id="userTable">
				<tbody>
					<tr>
						<th>Name</th>
						<th>User Type</th>
						<th></th>
					</tr>
					{{#each results}}
					<tr>
						<td>{{this.Name}}</td>
						<td>{{this.Type}}</td>
					</tr>
					{{/each}}
				</tbody>
			</table>
		{{/if}}
	</div>
</div>
